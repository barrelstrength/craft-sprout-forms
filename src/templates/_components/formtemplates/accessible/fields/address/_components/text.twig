<div id="{{ name }}-{{ inputName }}" class="{{ name }}-field {{ name }}-{{ inputName }}-field sproutformsaddress-text-field{% if class is defined %} {{ class }}{% endif %}">
    <div class="heading">
        <label>{{ label }}</label>
    </div>
    <div class="input">
        <input type="text" name="{{ name }}[{{ inputName }}]" value="{{ value }}"
            {%- if autocomplete is defined %} autocomplete="{{ autocomplete }}"{% endif -%}
        />
    </div>
</div>
